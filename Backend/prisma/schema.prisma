
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Role {
  User
  Admin
}

model User {
  id        Int  @id @default(autoincrement())
  email     String   @unique
  name      String
  password  String 
  role    Role @default(User)
}